{
  "name": "midi-demo",
  "displayName": "midi-demo",
  "description": "MIDI extension demo",
  "version": "0.0.0",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:midi-demo.info",
    "onCommand:midi-demo.note",
    "onCommand:midi-demo.scale"
  ],
  "browser": "./browser.js",
  "main": "./main.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "midi-demo.player",
        "displayName": "MIDI Player",
        "selector": [
          {
            "filenamePattern": "*.mid"
          },
          {
            "filenamePattern": "*.midi"
          }
        ]
      }
    ],
    "commands": [
      {
        "command": "midi-demo.info",
        "title": "Info",
        "category": "MIDI Demo"
      },
      {
        "command": "midi-demo.note",
        "title": "Play Note",
        "category": "MIDI Demo"
      },
      {
        "command": "midi-demo.scale",
        "title": "Play Scale",
        "category": "MIDI Demo"
      }
    ]
  },
  "scripts": {
    "browser": "vscode-test-web --extensionDevelopmentPath=. .",
    "compile": "webpack",
    "lint": "eslint .",
    "test": "node ./test/runTest.js"
  },
  "dependencies": {
    "jazz-midi-vscode": "^0.0.9",
    "jzz": "^1.5.9",
    "jzz-gui-player": "^1.6.1",
    "jzz-input-kbd": "^1.2.5",
    "jzz-midi-smf": "^1.7.1",
    "jzz-synth-tiny": "^1.3.3"
  },
  "devDependencies": {
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.18",
    "@types/vscode": "^1.74.0",
    "@vscode/test-electron": "^2.2.2",
    "@vscode/test-web": "^0.0.34",
    "eslint": "^8.32.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jazz-soft/jazz-midi-vscode.git"
  }
}
