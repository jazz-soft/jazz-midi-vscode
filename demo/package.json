{
  "name": "midi-demo",
  "displayName": "midi-demo",
  "description": "MIDI extension demo",
  "version": "0.0.0",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "browser": "./browser.js",
  "main": "./main.js",
  "contributes": {
    "views": {
      "explorer": [
        {
          "type": "webview",
          "id": "midi-demo.piano",
          "name": "Piano"
        },
        {
          "id": "midi-demo.tree",
          "name": "MIDI"
        }
      ]
    },
    "customEditors": [
      {
        "viewType": "midi-demo.player",
        "displayName": "MIDI Player",
        "selector": [
          {
            "filenamePattern": "*.mid"
          },
          {
            "filenamePattern": "*.midi"
          }
        ]
      }
    ],
    "commands": [
      {
        "command": "midi-demo.info",
        "title": "Info",
        "category": "MIDI Demo"
      },
      {
        "command": "midi-demo.note",
        "title": "Play Note",
        "category": "MIDI Demo"
      },
      {
        "command": "midi-demo.scale",
        "title": "Play Scale",
        "category": "MIDI Demo"
      }
    ]
  },
  "scripts": {
    "browser": "vscode-test-web --extensionDevelopmentPath=. .",
    "compile": "webpack",
    "lint": "eslint .",
    "test": "node ./test/runTest.js"
  },
  "dependencies": {
    "jazz-midi-vscode": "^1.0.5",
    "jzz": "^1.7.4",
    "jzz-gui-player": "^1.6.9",
    "jzz-gui-select": "^1.1.1",
    "jzz-input-kbd": "^1.2.8",
    "jzz-midi-smf": "^1.8.1",
    "jzz-synth-tiny": "^1.3.7"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.0",
    "@types/vscode": "^1.84.2",
    "@vscode/test-electron": "^2.3.8",
    "@vscode/test-web": "^0.0.48",
    "eslint": "^8.54.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "typescript": "^5.3.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jazz-soft/jazz-midi-vscode.git"
  }
}
