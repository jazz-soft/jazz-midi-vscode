{
  "name": "midi-demo",
  "displayName": "midi-demo",
  "description": "MIDI extension demo",
  "version": "0.0.0",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "browser": "./browser.js",
  "main": "./main.js",
  "contributes": {
    "views": {
      "explorer": [
        {
          "type": "webview",
          "id": "midi-demo.piano",
          "name": "Piano"
        },
        {
          "id": "midi-demo.tree",
          "name": "MIDI"
        }
      ]
    },
    "customEditors": [
      {
        "viewType": "midi-demo.player",
        "displayName": "MIDI Player",
        "selector": [
          {
            "filenamePattern": "*.mid"
          },
          {
            "filenamePattern": "*.midi"
          }
        ]
      }
    ],
    "commands": [
      {
        "command": "midi-demo.info",
        "title": "Info",
        "category": "MIDI Demo"
      },
      {
        "command": "midi-demo.note",
        "title": "Play Note",
        "category": "MIDI Demo"
      },
      {
        "command": "midi-demo.scale",
        "title": "Play Scale",
        "category": "MIDI Demo"
      }
    ]
  },
  "scripts": {
    "browser": "vscode-test-web --extensionDevelopmentPath=. .",
    "compile": "webpack",
    "lint": "eslint .",
    "test": "node ./test/runTest.js"
  },
  "dependencies": {
    "jazz-midi-vscode": "^1.0.7",
    "jzz": "^1.9.2",
    "jzz-gui-player": "^1.7.7",
    "jzz-gui-select": "^1.1.7",
    "jzz-input-kbd": "^1.3.4",
    "jzz-midi-smf": "^1.9.7",
    "jzz-synth-tiny": "^1.4.2"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.13.14",
    "@types/vscode": "^1.98.0",
    "@vscode/test-electron": "^2.4.1",
    "@vscode/test-web": "^0.0.68",
    "eslint": "^9.23.0",
    "glob": "^11.0.1",
    "mocha": "^11.1.0",
    "typescript": "^5.8.2",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jazz-soft/jazz-midi-vscode.git"
  }
}
