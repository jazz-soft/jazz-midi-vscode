{
  "name": "midi-demo",
  "displayName": "midi-demo",
  "description": "MIDI extension demo",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:midi-demo.piano"
  ],
  "browser": "./browser.js",
  "main": "./main.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "midi-demo.player",
        "displayName": "MIDI Player",
        "selector": [
          {
            "filenamePattern": "*.mid"
          }
        ]
      }
    ],
    "commands": [
      {
        "command": "midi-demo.piano",
        "title": "Piano",
        "category": "MIDI Demo"
      }
    ]
  },
  "scripts": {
    "browser": "vscode-test-web --extensionDevelopmentPath=. .",
    "compile": "webpack",
    "lint": "eslint .",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.18",
    "@types/vscode": "^1.74.0",
    "@vscode/test-electron": "^2.2.1",
    "@vscode/test-web": "^0.0.33",
    "eslint": "^8.31.0",
    "glob": "^8.0.3",
    "mocha": "^10.2.0",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}
