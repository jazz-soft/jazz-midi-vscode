{
  "name": "midi-demo",
  "displayName": "midi-demo",
  "description": "MIDI extension demo",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:midi-demo.piano"
  ],
  "browser": "./browser.js",
  "main": "./main.js",
  "contributes": {
    "commands": [
      {
        "command": "midi-demo.piano",
        "title": "Piano",
